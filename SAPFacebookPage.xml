<Module specificationVersion="2">
<ModulePrefs title="Facebook Page" description="Facebook OpenSocial Gadget" author="Deloitte" height="350"> </ModulePrefs>
<Content type="html">
<![CDATA[ 
<div id="fb-root"></div>
<script>(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = 'https://connect.facebook.net/en_US/sdk.js#xfbml=1&version=v3.2';
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>
<div class="fb-page" 
  data-href="https://www.facebook.com/marineatlanticferries"
  data-width="320"
  data-height="350"
  data-adapt-container-width="true"
  data-adapt-container-height="true"
  data-hide-cover="false"
  data-show-facepile="false"></div>
